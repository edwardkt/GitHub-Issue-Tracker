{
    "manifest_version": 2,
	
    "name": "GitHub Issue Tracker",
    "version": "1.0",
    "description": "Extension to track the latest github issue visits",
    
	"browser_action": 
	{
		"default_popup": "view.html",
		"default_icon": "images/git.png"
    },
	
	"background": 
	{
	   "scripts": ["event.js"],
	   "persistent": false
	},
	
	"permissions":
	[
		"history"
	]
}